# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
*.log
/log/*
!/log/.keep
/tmp
/vendor/bundler/
/.pry_history
/doc/*.yml
*~
.*.sw*
/dist/*
/db/*.sqlite3
/.ruby-version
